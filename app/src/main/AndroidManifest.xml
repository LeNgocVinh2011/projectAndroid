<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_rtsp">
<!--    permission-->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/img"
        android:label="@string/app_name"
        android:roundIcon="@drawable/img"
        android:supportsRtl="true"
        android:theme="@style/Theme.App_rtsp">
        <activity android:name=".Activity.AddActivity" android:label="@string/add_layout" android:parentActivityName=".Activity.MainActivity" />
        <activity android:name=".Activity.SettingActivity" android:label="@string/setting" android:parentActivityName=".Activity.MainActivity" />
        <activity android:name=".Activity.VideoView" android:configChanges="orientation|screenSize" android:theme="@style/Theme.Design.NoActionBar" android:label="@string/video_view" android:parentActivityName=".Activity.MainActivity" />
        <activity android:name=".Activity.About" android:label="@string/about_view" android:parentActivityName=".Activity.SettingActivity" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>